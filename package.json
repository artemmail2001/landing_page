      "js": "dist/js",
      "fonts": "dist/fonts",
      "img": "dist/img"
    },
    "libs": {
      "js": "src/js/libs.js",
      "css": "src/css/libs.css"
    }
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
