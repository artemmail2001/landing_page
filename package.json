    "run-sequence": "^2.2.1"
  },
  "scripts": {
    "start": "gulp",
    "dev": "gulp"
  },
  "config": {
    "src": {
      "sass": "src/sass/main.sass",
      "html": "src/html/index.html",
      "js": "src/js/main.js",
      "fonts": "src/fonts/**/*.*",
      "img": {
        "compress": [
